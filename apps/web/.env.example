APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Request logging toggles
# Set to true/false to force enable/disable. When unset, defaults to enabled in non-production.
LOG_REQUESTS=
LOG_SSE_PROGRESS=

DB_CONNECTION=pgsql
DB_HOST=127.0.0.1
DB_PORT=5432
DB_DATABASE=content_api
DB_USERNAME=postgres
DB_PASSWORD=postgres

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=

BROADCAST_CONNECTION=reverb

# Laravel Reverb (WebSockets)
REVERB_APP_ID=content-local
REVERB_APP_KEY=local-app-key
REVERB_APP_SECRET=local-app-secret
REVERB_HOST=localhost
# Set this when the API runs in containers so it can reach the Reverb service by its internal hostname
REVERB_INTERNAL_HOST=
REVERB_PORT=8080
REVERB_SCHEME=http
# Comma-separated list of allowed WebSocket origins for Reverb. In production, set to your site origins.
# Example: https://voxprismatic.com,https://www.voxprismatic.com
REVERB_ALLOWED_ORIGINS=
REVERB_SERVER_HOST=0.0.0.0
REVERB_SERVER_PORT=8080
REVERB_SERVER_PATH=
REVERB_SCALING_ENABLED=false

# Optional fallback: legacy Pusher-compatible configuration
# PUSHER_APP_ID=local
# PUSHER_APP_KEY=app-key
# PUSHER_APP_SECRET=app-secret
# PUSHER_APP_CLUSTER=mt1
# PUSHER_HOST=localhost
# PUSHER_PORT=6001
# PUSHER_SCHEME=http
# PUSHER_USE_TLS=false
FILESYSTEM_DISK=local
QUEUE_CONNECTION=redis
REDIS_QUEUE=processing
REDIS_QUEUE_CONNECTION=default

CACHE_STORE=database
# CACHE_PREFIX=

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
VITE_REVERB_APP_KEY="${REVERB_APP_KEY}"
VITE_REVERB_HOST="${REVERB_HOST}"
VITE_REVERB_PORT="${REVERB_PORT}"
VITE_REVERB_SCHEME="${REVERB_SCHEME}"

# LinkedIn OAuth
LINKEDIN_CLIENT_ID=
LINKEDIN_CLIENT_SECRET=
LINKEDIN_REDIRECT_URI=http://localhost/linkedin/callback

# Stripe
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_ID=
STRIPE_SUCCESS_URL=http://localhost/billing/success
STRIPE_CANCEL_URL=http://localhost/billing/cancel
STRIPE_BILLING_PORTAL_RETURN_URL=http://localhost/settings/billing

# Gemini pricing overrides (USD per 1M tokens)
GEMINI_25_PRO_PROMPT_PER_1M=1.25
GEMINI_25_PRO_COMPLETION_PER_1M=10
GEMINI_25_FLASH_PROMPT_PER_1M=0.3
GEMINI_25_FLASH_COMPLETION_PER_1M=2.5
GEMINI_DEFAULT_PROMPT_PER_1M=1.25
GEMINI_DEFAULT_COMPLETION_PER_1M=10
